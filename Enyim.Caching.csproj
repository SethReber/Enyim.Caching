<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.0</TargetFramework>
		<AllowUnsafeBlocks>true</AllowUnsafeBlocks>
		<RootNamespace>Enyim</RootNamespace>
		<AssemblyName>Enyim.Caching</AssemblyName>
		<AssemblyTitle>Enyim Memcached</AssemblyTitle>
		<AssemblyVersion>1.5.1.1806</AssemblyVersion>
		<FileVersion>1.5.1.1806</FileVersion>
		<InformationalVersion>v1.5.netstandard-2+rev:2018.05.17</InformationalVersion>
		<Product>Enyim Memcached</Product>
		<Company>VIEApps.net</Company>
		<GeneratePackageOnBuild>false</GeneratePackageOnBuild>
		<PackageId>VIEApps.Enyim.Caching</PackageId>
		<Version>1.5.1.1806</Version>
		<SignAssembly>false</SignAssembly>
		<DelaySign>false</DelaySign>
		<AssemblyOriginatorKeyFile>../VIEApps.Components.snk</AssemblyOriginatorKeyFile>
		<Title>Enyim Memcached</Title>
		<Description>The memcached client library, fully async with various transcoders (binary formatter, bson, protobuf, msgpack) on .NET Standard 2.0</Description>
		<Authors>VIEApps.net</Authors>
		<Copyright>© 2010 Attila Kiskó, © 2016 CNBlogs, © 2018 VIEApps.net</Copyright>
		<PackageTags>cache;caching;distributed;distributedcache;memcached;enyim;enyimmemcached;enyim.memcached;enyim.caching;vieapps;vieapps.components</PackageTags>
		<PackageReleaseNotes>Add NoDelay option to socket</PackageReleaseNotes>
		<PackageProjectUrl>https://github.com/vieapps/Enyim.Caching</PackageProjectUrl>
		<RepositoryUrl>https://github.com/vieapps/Enyim.Caching</RepositoryUrl>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
		<WarningLevel>3</WarningLevel>
		<NoWarn>1701;1702;1705;1066;0067</NoWarn>
		<DocumentationFile>bin\Debug\netstandard2.0\Enyim.Caching.xml</DocumentationFile>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
		<WarningLevel>3</WarningLevel>
		<NoWarn>1701;1702;1705;1066;0067</NoWarn>
		<DocumentationFile>bin\Release\netstandard2.0\Enyim.Caching.xml</DocumentationFile>
	</PropertyGroup>

	<ItemGroup>
		<None Remove=".DS_Store" />
		<None Remove=".gitattributes" />
		<None Remove=".gitignore" />
		<None Remove="LICENSE.md" />
		<None Remove="README.md" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.AspNetCore.Http.Abstractions" Version="2.0.3" />
		<PackageReference Include="Microsoft.Extensions.Caching.Abstractions" Version="2.0.2" />
		<PackageReference Include="Microsoft.Extensions.Configuration.Abstractions" Version="2.0.2" />
		<PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="2.0.0" />
		<PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="2.0.2" />
		<PackageReference Include="Microsoft.Extensions.Options" Version="2.0.2" />
		<PackageReference Include="Microsoft.Extensions.Options.ConfigurationExtensions" Version="2.0.2" />
		<PackageReference Include="System.Configuration.ConfigurationManager" Version="4.4.1" />
	</ItemGroup>

</Project>